    "@types/react-dom": "18.3.0",
    "@types/react-syntax-highlighter": "15.5.13",
    "cspell": "8.10.4",
    "eslint": "8.57.0",
    "eslint-config-sc-all": "0.0.4",
    "eslint-config-sc-jest": "0.0.4",
    "eslint-config-sc-next": "0.0.2",
    "eslint-config-sc-storybook": "0.0.4",
    "eslint-config-sc-ts": "0.0.6",
    "eslint-plugin-jsdoc": "48.7.0",
    "eslint-plugin-next-on-pages": "1.12.1",
    "eslint-plugin-sc-js": "0.0.4",
    "eslint-plugin-sonarjs": "1.0.3",
