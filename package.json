{
  "name": "project-strict-check-homepage",
  "license": "UNLICENSED",
  "author": "akky-xxxx",
  "engines": {
    "node": "20.15.1",
    "npm": "pnpmを使用してください",
    "yarn": "pnpmを使用してください"
  },
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "check-code": "npm-run-all -p lint spell-check type-check -s test-jest",
    "deploy": "$npm_execpath run pages:build && wrangler pages deploy",
    "dev": "next dev",
    "fix": "run-p -c \"fix:*\"",
    "fix:config": "eslint -c eslint.config.js.mjs \"./\" --max-warnings 0 --fix",
    "fix:pack": "fixpack",
    "fix:prettier": "prettier \"./*.{json,toml,yaml,yml}\" \"./**/*.{json,toml,yaml,yml}\" \"!pnpm-lock.yaml\" -w -u",
    "fix:product-code": "eslint \"src/*/*.ts{,x}\" \"src/**/*.ts{,x}\" --max-warnings 0 --fix",
    "lint": "run-p -c \"lint:*\"",
    "lint:config": "eslint -c eslint.config.js.mjs \"./\" --max-warnings 0",
    "lint:pack": "fixpack --dryRun",
    "lint:prettier": "prettier \"./*.{json,toml,yaml,yml}\" \"./**/*.{json,toml,yaml,yml}\" \"!pnpm-lock.yaml\" -c -u",
    "lint:product-code": "eslint \"src/*/*.ts{,x}\" \"src/**/*.ts{,x}\" --max-warnings 0",
    "log": "wrangler pages deployment tail --project-name strict-check-series --environment preview",
    "pages:build": "npx @cloudflare/next-on-pages",
    "prepare": "run-s \"prepare:*\"",
    "prepare:huksy": "husky",
    "prepare:panda": "panda codegen",
    "prepare:playwright": "pnpm exec playwright install",
    "preview": "$npm_execpath run pages:build && wrangler pages dev",
    "spell-check": "cspell lint --no-progress \"./**/*.{js,mjs,ts,tsx}\" \"./*.{js,mjs}\"",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test-jest": "jest --coverage",
    "test-storybook": "test-storybook",
    "type-check": "tsc"
  },
  "dependencies": {
    "js-cookie": "3.0.5",
    "next": "14.2.5",
    "next-extra": "0.4.1",
    "preact": "10.22.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-syntax-highlighter": "15.5.0",
    "valibot": "0.36.0",
    "zustand": "4.5.4"
