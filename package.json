{
  "name": "project-strict-check-homepage",
  "license": "UNLICENSED",
  "author": "akky-xxxx",
  "engines": {
    "node": "20.15.0",
    "npm": "pnpmを使用してください",
    "yarn": "pnpmを使用してください"
  },
  "scripts": {
    "build": "next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "check-code": "npm-run-all -p lint spell-check type-check",
    "deploy": "$npm_execpath run pages:build && wrangler pages deploy",
    "dev": "next dev",
    "fix": "run-p -c \"fix:*\"",
    "fix:config": "eslint -c eslint.config.js.mjs \"./\" --max-warnings 0 --fix",
    "fix:pack": "fixpack",
    "fix:prettier": "prettier \"./*.json\" \"./**/*.json\" -w -u",
    "fix:product-code": "eslint \"src/*/*.ts{,x}\" \"src/**/*.ts{,x}\" --max-warnings 0 --fix",
    "lint": "run-p -c \"lint:*\"",
    "lint:config": "eslint -c eslint.config.js.mjs \"./\" --max-warnings 0",
    "lint:pack": "fixpack --dryRun",
    "lint:prettier": "prettier \"./*.json\" \"./**/*.json\" -c -u",
    "lint:product-code": "eslint \"src/*/*.ts{,x}\" \"src/**/*.ts{,x}\" --max-warnings 0",
    "log": "wrangler pages deployment tail --project-name strict-check-series --environment preview",
    "pages:build": "npx @cloudflare/next-on-pages",
    "prepare": "run-s \"prepare:*\"",
    "prepare:huksy": "husky",
    "prepare:panda": "panda codegen",
    "preview": "$npm_execpath run pages:build && wrangler pages dev",
    "spell-check": "cspell lint --no-progress \"./**/*.{js,mjs}\" \"./*.{js,mjs}\"",
    "start": "next start",
    "type-check": "tsc"
  },
  "dependencies": {
    "next": "14.2.4",
    "preact": "10.22.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.12.0",
    "@cloudflare/workers-types": "4.20240620.0",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/eslintrc": "3.1.0",
    "@pandacss/dev": "0.41.0",
    "@tsconfig/next": "2.0.3",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "cspell": "8.9.1",
    "eslint": "8.57.0",
    "eslint-config-sc-all": "0.0.1",
    "eslint-config-sc-jest": "0.0.3",
    "eslint-config-sc-next": "0.0.1",
    "eslint-config-sc-storybook": "0.0.3",
    "eslint-config-sc-ts": "0.0.3",
    "eslint-plugin-jsdoc": "48.5.0",
    "eslint-plugin-next-on-pages": "1.12.0",
    "eslint-plugin-sc-js": "0.0.3",
    "eslint-plugin-sonarjs": "1.0.3",
    "fixpack": "4.0.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "npm-run-all2": "6.2.0",
    "postcss": "8.4.38",
    "prettier": "3.3.2",
    "typescript": "5.5.2",
    "typescript-eslint": "7.14.1",
    "vercel": "34.2.8",
    "wrangler": "3.62.0"
  },
  "version": "0.0.1",
  "packageManager": "pnpm@9.4.0"
}
